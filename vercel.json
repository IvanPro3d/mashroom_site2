{
    "version": 2,
    "buildCommand": null,
    "devCommand": null,
    "installCommand": null,
    "framework": null,
    "outputDirectory": ".",
    "public": true,
    "rewrites": [
      { "source": "/(.*)", "destination": "/$1" }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/html; charset=utf-8"
          }
        ]
      },
      {
        "source": "/(.*).css",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/css"
          }
        ]
      }
    ]
  }